{
  "name": "cprog-book",
  "private": true,
  "description": "Book about programming on C/C++",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build && yarn del-maps",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "del-maps": "rimraf ./public/**.map",
    "deploy": "netlify deploy --dir=public --prod",
    "lint": "eslint . --ext md,mdx",
    "prettier": "prettier --write ./content"
  },
  "browserslist": [
    ">0.25%",
    "not dead",
    "not ie <= 11",
    "not android <= 4.4.4",
    "not edge < 15",
    "not op_mini all",
    "not iOS <= 10.2"
  ],
  "dependencies": {
    "@linaria/core": "^4.1.2",
    "@linaria/react": "^4.1.3",
    "@matejmazur/react-katex": "^3.1.3",
    "@mdx-js/react": "^2.3.0",
    "@svgr/webpack": "^8.0.1",
    "gatsby": "^5.10.0",
    "gatsby-plugin-linaria": "^4.0.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-mdx": "^5.10.0",
    "gatsby-plugin-offline": "^6.10.0",
    "gatsby-plugin-react-helmet": "^6.10.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^6.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-sitemap": "^6.10.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-remark-images": "^7.10.0",
    "gatsby-remark-prismjs": "^7.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "katex": "^0.16.7",
    "linaria": "^4.1.3",
    "nprogress": "^0.2.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-helmet": "^6.1.0",
    "react-use": "^17.4.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-mdx": "^2.0.5",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.1"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  }
}
